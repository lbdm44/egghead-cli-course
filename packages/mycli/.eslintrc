{
  "extends": ["oclif", "oclif-typescript"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error"
  },
  "overrides": [
    {
      "files": ["**/*.{js,ts}", "./bin/run"],
      "rules": {
        "object-curly-spacing": 0,
        "semi": "off",
        "indent": "off"
      }
    }
  ]
}
